get_parameters <- function(stanfits){
for (i_m in range(length(stanfits))){
fit <- stanfits[i_m]
model_name <- fit@model_name
model_params <- fit@model_pars[1:5]
param_df < rstan::summary(fit, pars = model_params)$summary[,c("mean", "se_mean")]
print(param_df)
}
}
